@article{cooperBillIntroductionNineteenth1989,
	title = {Bill {Introduction} in the {Nineteenth} {Century}: {A} {Study} of {Institutional} {Change}},
	volume = {14},
	issn = {0362-9805},
	shorttitle = {Bill {Introduction} in the {Nineteenth} {Century}},
	url = {https://www.jstor.org/stable/440092},
	doi = {10.2307/440092},
	abstract = {Current procedures for bill introduction in the U.S. House were neither originally intended nor quickly attained. They required almost a century to emerge and were tied to basic structural changes in a number of related areas. Thus, to understand the evolution of bill introduction is to understand the evolution of many of the defining institutional features of the modern House. Viewed more theoretically, analysis of this development has benefits that extend beyond better understanding of the dynamics of a particular legislature. It provides an excellent case study for clarifying and examining basic differences in micro and macro approaches to institutional analysis.},
	number = {1},
	urldate = {2019-08-29},
	journal = {Legislative Studies Quarterly},
	author = {Cooper, Joseph and Young, Cheryl D.},
	year = {1989},
	note = {Citation Key Alias: cooperBillIntroductionNineteenth1989},
	pages = {67--105}
}

@article{kernellUnderstanding19thCentury1977,
	title = {Toward {Understanding} 19th {Century} {Congressional} {Careers}: {Ambition}, {Competition}, and {Rotation}},
	volume = {21},
	issn = {0092-5853},
	shorttitle = {Toward {Understanding} 19th {Century} {Congressional} {Careers}},
	url = {https://www.jstor.org/stable/2110731},
	doi = {10.2307/2110731},
	abstract = {The growing careerism of congressmen at the turn of the century has been widely viewed as a chief cause for the modernization of the House of Representatives. Thus, a prominent concern of recent research in congressional development has been the reasons for career development at that time. In this paper I attempt to distinguish the effects of three contributors to the nineteenth century turnover: competition, rotation, and ambition. After analyzing six decades of nineteenth-century congressional elections, I found that all three variables served to reduce turnover toward the end of the century, but that the decline in voluntary retirement, reflecting political ambition, was the primary contributor to increased membership stability.},
	number = {4},
	urldate = {2019-08-29},
	journal = {American Journal of Political Science},
	author = {Kernell, Samuel},
	year = {1977},
	pages = {669--693}
}

@article{mansbridgeWhatPoliticalScience2014,
	title = {What {Is} {Political} {Science} {For}?},
	volume = {12},
	issn = {1537-5927, 1541-0986},
	url = {http://www.cambridge.org/core/journals/perspectives-on-politics/article/what-is-political-science-for/703BD2EA6ABF000BF1FD3ABF80BA2E11},
	doi = {10.1017/S153759271300368X},
	abstract = {This address advances three ideas. First, political science as a discipline has a mandate to help human beings govern themselves. Second, within this mandate we should be focusing, more than we do now, on creating legitimate coercion. In a world of increasing interdependence we now face an almost infinite number of collective action problems created when something we need or want involves a “free-access good.” We need coercion to solve these collective action problems. The best coercion is normatively legitimate coercion. Democratic theory, however, has focused more on preventing tyranny than on how to legitimate coercion. Finally, our discipline has neglected an important source of legitimate coercion: negotiation to agreement. Recognizing the central role of negotiation in politics would shed a different light on our relatively unexamined democratic commitments to transparency in process and contested elections. This analysis is overall both descriptive and aspirational, arguing that helping human beings to govern themselves has been in the DNA of our profession since its inception.},
	language = {en},
	number = {1},
	urldate = {2019-08-29},
	journal = {Perspectives on Politics},
	author = {Mansbridge, Jane},
	month = mar,
	year = {2014},
	note = {Citation Key Alias: mansbridgeWhatPoliticalScience2014},
	pages = {8--17},
	file = {Snapshot:D\:\\Zotero\\storage\\C44STU5Y\\703BD2EA6ABF000BF1FD3ABF80BA2E11.html:text/html;Full Text PDF:D\:\\Zotero\\storage\\VPGGFWTB\\Mansbridge - 2014 - What Is Political Science For.pdf:application/pdf;Full Text PDF:D\:\\Zotero\\storage\\4QBW7VK5\\Mansbridge - 2014 - What Is Political Science For.pdf:application/pdf}
}

